package com.insiteo.lbs.common.utils.geometry;

import android.os.Parcel;
import android.os.Parcelable;


/**
 * An extension of a Point object : it contains an ID of map and a point. 
 * This class is Parcelable.
 */
public class Position implements Parcelable {
	protected int mMapID;		// map ID
	private PointD mCoord;	// the x/y pair in meter

	/**
	 * Constructor for the Position object.
	 * @param aMapID	The ID of the map where this Location is valid.
	 * @param aX		The X coordinate, relative to the linked map's origin, expressed in meters.
	 * @param aY		The Y coordinate, relative to the linked map's origin, expressed in meters.
	 */
	 public Position(int aMapID, double aX, double aY) {
		mMapID = aMapID;
		mCoord = new PointD(aX, aY);
	 }
	
	/**
	 * Constructor for the Position object, with a zero accuracy.
	 * @param aMapID	The ID of the map where this Position is valid.
	 * @param aCoord	The X/Y coordinate pair, relative to the linked map's origin, expressed in meters.
	 */
	public Position(int aMapID, PointD aCoord) {
		this(aMapID, aCoord.x, aCoord.y);
	}	
	
	/**
	 * Copy-constructor for the Position object.
	 * @param aPosition	The Position object to duplicate.
	 */
	public Position(Position aPosition) {
		this(aPosition.getMapID(), aPosition.getCoord());
	}
	
	
	/**
	 * Copy values from aPosition. No allocation is done. 
	 * @param aPosition the Position to copy values from.
	 */
	public void setPosition(Position aPosition) {
		mMapID = aPosition.mMapID;
		mCoord.set(aPosition.mCoord);
	}
	

	/**
	 * Gets the Map unique identifier from this Position object.
	 * @return	A unique MapID.
	 */
	public int getMapID() {
		return mMapID;
	}
	
	/**
	 * Gets the X/Y pair from this Position object.
	 * @return the x/y pair of this position
	 */
	public PointD getCoord() {
		return mCoord;
	}
	
	/**
	 * Get the X value from this Position object
	 * @return the x of this position
	 */
	public double getX() {
		return mCoord.x;
	}
	
	/**
	 * Get the Y value from this Position object
	 * @return the y of this position
	 */
	public double getY() {
		return mCoord.y;
	}
	
	
	/**
	 * Set position to the given position
	 * @param pos the position which to copy values from
	 */
	public void set(Position pos) {
		mMapID = pos.getMapID();
		mCoord.set(pos.getCoord().x, pos.getCoord().y);
	}
	
	/**
	 * Set map id
	 * @param map map's id to set
	 */
	public void setMap(int map) {
		mMapID = map;
	}
	
	/**
	 * Set x/y pair
	 * @param coord x/y pair to set
	 */
	public void setCoord(PointD coord) {
		mCoord.set(coord.x, coord.y);
	}
	
	/**
	 * Set x value
	 * @param x the value to set
	 */
	public void setX(double x) {
		mCoord.set(x, mCoord.y);
	}
	
	/**
	 * Set y value
	 * @param y the value to set
	 */
	public void setY(double y) {
		mCoord.set(mCoord.x, y);
	}
	
	
	/**
	 * Test if given object is equal to Position. 
	 * Two Positions are equal if their map ID and coordinates are equal
	 * @param o object on which to test equality
	 * return true if this instance of Position is equal to given object
	 */
	@Override
    public boolean equals(Object o) {
        if (!(o instanceof Position)) {
            return false;
        }
        Position pos = (Position) o;
        
        return (pos.getMapID() == mMapID) 
        	&amp;&amp; (pos.getCoord().equals(mCoord));
    }

	@Override
	public int describeContents() {
		return 0;
	}

	@Override
	public void writeToParcel(Parcel dest, int flags) {
		dest.writeInt(mMapID);
		dest.writeParcelable(mCoord, PARCELABLE_WRITE_RETURN_VALUE);
	}
	
	/**
	 * Set position coordinates, from the data stored in the specified parcel
	 * @param source The parcel to read the coordinates from 
	 */
	public void readFromParcel(Parcel source) {
		mMapID = source.readInt();
		mCoord = source.readParcelable(PointD.class.getClassLoader());
	}
	
	public static final Parcelable.Creator&lt;Position&gt; CREATOR = new Parcelable.Creator&lt;Position&gt;() {
		public Position createFromParcel(Parcel source) {
            Position pos = new Position(0, 0, 0);
            pos.readFromParcel(source);
            return pos;
        }
        public Position[] newArray(int size) {
            return new Position[size];
        }
	};

	@Override
	public String toString() {
		String value = " Position(mapID = " + mMapID + ", x = " + getX() + ", y = " + getY() +")";
		return value;
	} 
	
	
}

