package com.insiteo.lbs.common.utils.geometry;

import android.os.Parcel;
import android.os.Parcelable;


/**
 * A simple point in double precision.
 * This point is Parcelable.
 */
public class PointD implements Parcelable {
	/** x coordinate of the point */
	public double x = 0d;
	/** y coordinate of the point */
	public double y = 0d;
	
	/**
	 * Empty constructor (x=0, y=0)
	 */
	public PointD() {
	}
	
	/**
	 * Constructor
	 * @param ax x value
	 * @param ay y value
	 */
	public PointD(double ax, double ay) {
		x = ax;
		y = ay;
	}
	
	/**
	 * Copy constructor
	 * @param point the point to copy
	 */
	public PointD(PointD point) {
		x = point.x;
		y = point.y;
	}

	/**
	 * Set x and y in the point
	 * @param point the point to set values from
	 */
	public void set(PointD point) {
		x = point.x;
		y = point.y;
	}
	
	/**
	 * Set x and y in the point
	 * @param ax x value
	 * @param ay y value
	 */
	public void set(double ax, double ay) {
		x = ax;
		y = ay;
	}
	
	/**
	 * Offset the point
	 * @param offx the offset to apply to x
	 * @param offy the offset to apply to y
	 */
	public void offset(double offx, double offy) {
		x += offx;
		y += offy;
	}
	
	@Override
	public boolean equals(Object o) {
		boolean isEqual = false;
        if (this == o) {
        	isEqual = true;
        }
        else {
			if ((o instanceof PointD)) {
				PointD p = (PointD) o;
	            isEqual = (p.x == x &amp;&amp; p.y == y);
			}
        }
		return isEqual;
	}

	@Override
	public int describeContents() {
		return 0;
	}

	@Override
	public void writeToParcel(Parcel dest, int flags) {
		dest.writeDouble(x);
		dest.writeDouble(y);
	}
	
	/**
	 * Set point coordinates, from the data stored in the specified parcel
	 * @param source The parcel to read the coordinates from 
	 */
	public void readFromParcel(Parcel source) {
		x = source.readDouble();
		y = source.readDouble();
	}
	
	public static final Parcelable.Creator&lt;PointD&gt; CREATOR = new Parcelable.Creator&lt;PointD&gt;() {
		public PointD createFromParcel(Parcel source) {
            PointD p = new PointD();
            p.readFromParcel(source);
            return p;
        }
        public PointD[] newArray(int size) {
            return new PointD[size];
        }
	}; 

	public String toString(){
		return "(" + x + ", " + y + ")";
	}
}

