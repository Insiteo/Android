package com.insiteo.lbs.common.auth.parsers;

import com.insiteo.lbs.common.parser.IRequestEncoder;
import com.insiteo.lbs.common.utils.EncodeUtils;
import com.insiteo.lbs.protobuf.dispatch.request.auth.ProtoReqLoginRoot.PBReqLoginRoot;

public class LoginRequestEncoder implements IRequestEncoder {

	private String mLogin = null;
	private String mPassword = null;
	

	public LoginRequestEncoder(String aLogin, String aPassword) {
		mLogin = aLogin;
		mPassword  = aPassword;
	}
	
	@Override
	public byte[] encodeBinary() {
		byte[] req = null;
		PBReqLoginRoot.Builder message = PBReqLoginRoot.newBuilder();
		String md5 = EncodeUtils.getMD5SumStr(new String(mLogin + "1ns1te0$r0cKsS*!||#" + mPassword).getBytes());
		message.setLogin(mLogin);
		message.setPassword(md5);
		req = message.build().toByteArray();
		return req;
	}

}

